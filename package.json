{
  "name": "dappbooster",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:watch": "routes:watch & vite",
    "build": "tsc --noEmit && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "preview": "vite preview",
    "routes:generate": "tsr generate",
    "routes:watch": "tsr watch",
    "prepare": "husky",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "wagmi-generate": "wagmi generate --config src/lib/wagmi/config.ts",
    "subgraph-codegen": "graphql-codegen --config ./src/subgraphs/codegen.ts",
    "postinstall": "pnpm wagmi-generate && pnpm subgraph-codegen"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "stylelint --fix"
    ],
    "src/**/*.{js,ts,jsx,tsx,css,scss}": [
      "prettier --write --ignore-unknown"
    ],
    "src/**/*.{ts,tsx}": [
      "tsc-files --noEmit",
      "vitest related --run --coverage=false"
    ]
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@lifi/sdk": "^3.0.1",
    "@t3-oss/env-core": "^0.10.1",
    "@tanstack/react-query": "^5.36.0",
    "@tanstack/react-router": "^1.32.5",
    "@tanstack/react-virtual": "^3.7.0",
    "@uniswap/default-token-list": "^11.19.0",
    "connectkit": "^1.8.2",
    "db-ui-toolkit": "github:BootNodeDev/db-ui-toolkit",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "modern-normalize": "^2.0.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-hot-toast": "^2.4.1",
    "react-jazzicon": "^1.0.4",
    "styled-components": "^6.1.11",
    "use-debounce": "^10.0.1",
    "viem": "~2.10.9",
    "wagmi": "^2.9.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.2",
    "@parcel/watcher": "^2.4.1",
    "@tanstack/react-query-devtools": "^5.36.0",
    "@tanstack/router-cli": "^1.32.2",
    "@tanstack/router-devtools": "^1.32.5",
    "@tanstack/router-vite-plugin": "^1.32.2",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@wagmi/cli": "^2.1.13",
    "change-case": "^5.4.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.3.2",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.1",
    "tsc-files": "^1.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "pnpm": {
    "overrides": {
      "@csstools/media-query-list-parser": "2.1.12"
    }
  }
}