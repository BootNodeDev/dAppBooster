{
  "name": "dappvooster",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:watch": "routes:watch & vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "routes:generate": "tsr generate",
    "routes:watch": "tsr watch",
    "prepare": "husky"
